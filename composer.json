	"require": {
		"php": ">=5.6",
		"symfony/yaml": "^2.0",
		"mnapoli/front-yaml": "^1.5",
		"guzzlehttp/guzzle": "~6.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~5.6.8"
	},
	"config": {
		"process-timeout": 600
	},
	"bin": [
		"bin/elder2fs"
	],
  "autoload": {
    "psr-4": {
      "Elder\\": "src"
    }
  }
}
